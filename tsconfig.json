{
    "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "moduleResolution": "Node",
      "outDir": "dist",
      "declaration": true,
      "declarationDir": "dist",
      "emitDeclarationOnly": true,
      "jsx": "react-jsx",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "skipLibCheck": true
    },
    "include": ["src"]
  }
  